<!doctype html>
<html>

<head>
 <meta charset="utf-8" />
 <title>CodeSnips</title>
 <link rel="stylesheet" type="text/css" href="static/main.css" />
 <link rel="stylesheet" type="text/css" href="/static/main.css" />
</head>

<body>
 {{#code}}
 <h1>{{language}} Code Snippets:</h1>
 {{/code}}
 <div class="codeContainer">
  {{#code}}
   <div class="codeSnippets">
     <a href="{{_id}}"><h2>{{title}}</h2></a>
     <h3>{{author}}</h3>
     <code>{{body}}</code>
     <h4>{{notes}}</h4>
     <a href="{{language}}"><h5>{{language}}</h5></a>
     <h6>{{tags}}</h6>
   </div>
  {{/code}}
 </div>
</body>

</html>
